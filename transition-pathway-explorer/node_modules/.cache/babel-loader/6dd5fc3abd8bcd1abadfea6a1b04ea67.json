e needed above navigation levers\n    };\n  },\n  created: function created() {\n    this.selectableCountries = inputsService.selectableCountries;\n  },\n  methods: {\n    openCountrySelect: function openCountrySelect() {\n      // don't allow any change when countryZoom is disabled (during a long fetch)\n      if (this.appState.countryZoomDisabled) {\n        return;\n      }\n      this.$refs.countrySelect.toggleOpen(true);\n    },\n    selectCountry: function selectCountry(countr) {\n      // don't allow any change when countryZoom is disabled (during a long fetch)\n      if (this.appState.countryZoomDisabled) {\n        return;\n      }\n\n      // Mobile and Desktop\n      if (countr && countr.id && countr.id !== 'EU' && inputsService.countries[countr.id]) {\n        inputsService.setSelectedCountry(countr.id);\n      } else {\n        inputsService.unselectCountry();\n        inputsService.checkIfLeversMatchExamplePathway();\n      }\n\n      // Desktop only\n      if (!this.appState.isMobile) {\n        dataService.rewriteURL(dataService.updateURLParams());\n        dataService.debouncedFetch();\n\n        //this triggers the HighCharts built-in redraw event. Not a very elegant solution, but very short\n        helpers.triggerWindowResize();\n      }\n\n      // Mobile only\n      if (this.appState.isMobile) {\n        // send availability request\n        dataService.availabilityRequest();\n      }\n    }\n  }\n};",{"version":3,"sources":["LeversTable.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0CA,OAAA,KAAA,WAAA;AACA,OAAA,KAAA,aAAA;AACA,OAAA,KAAA,OAAA;AAEA,OAAA,MAAA;AACA,OAAA,QAAA;AACA,OAAA,WAAA;AAGA,eAAA;EACA,IAAA,EAAA,aAAA;EACA,UAAA,EAAA;IACA,MAAA,EAAA,MAAA;IAAA,QAAA,EAAA,QAAA;IAAA,WAAA,EAAA;EACA,CAAA;EACA,KAAA,EAAA,CAAA,UAAA,CAAA;EACA,IAAA,WAAA,KAAA,EAAA;IACA,OAAA;MACA,QAAA,EAAA,aAAA,CAAA,QAAA,CAAA;IACA,CAAA;EACA,CAAA;EACA,OAAA,WAAA,QAAA,EAAA;IACA,IAAA,CAAA,mBAAA,GAAA,aAAA,CAAA,mBAAA;EACA,CAAA;EACA,OAAA,EAAA;IACA,iBAAA,WAAA,k