components/LeverInfoWrapper.vue\";\nimport Modal from \"./lib/Modal.vue\";\nexport default {\n  name: 'App',\n  components: {\n    Header: Header,\n    Target: Target,\n    Intro: Intro,\n    Inputs: Inputs,\n    Outputs: Outputs,\n    Modal: Modal,\n    LeverInfoWrapper: LeverInfoWrapper,\n    'LegalInfo': function LegalInfo() {\n      return import(\"./components/LegalInfo.vue\");\n    },\n    'InputsMobile': function InputsMobile() {\n      return import(\"./components/InputsMobile.vue\");\n    },\n    'EmissionsBarMobile': function EmissionsBarMobile() {\n      return import(\"./components/EmissionsBarMobile.vue\");\n    }\n  },\n  data: function data() {\n    return {\n      // every existing object put inside here will be transformed into a reactive getter-setter construct\n      appState: dataService.appState,\n      howToOpen: false\n    };\n  },\n  methods: {\n    closeHowToModal: function closeHowToModal() {\n      this.howToOpen = false;\n    },\n    openHowToModal: function openHowToModal() {\n      this.howToOpen = true;\n    },\n    autoCloseMenu: function autoCloseMenu(event) {\n      if (!this.$refs.pagemenu.contains(event.target)) {\n        this.appState.menuOpen = false;\n        window.removeEventListener('click', this.autoCloseMenu);\n      }\n    },\n    toggleMenu: function toggleMenu(event) {\n      this.appState.menuOpen = !this.appState.menuOpen;\n\n      if (this.appState.menuOpen) {\n        if (event) {\n          //this function can also be called directly (not as an event handler)\n          event.stopPropagation();\n        }\n\n        window.addEventListener('click', this.autoCloseMenu);\n      } else {\n        window.removeEventListener('click', this.autoCloseMenu);\n      }\n    },\n    hideInputInfo: function hideInputInfo() {\n      inputsService.hideInputInfo();\n    }\n  }\n};",{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqFA,OAAA,KAAA,WAAA;AACA,OAAA,KAAA,aAAA;AACA,OAAA,MAAA;AACA,OAAA,MAAA;AACA,OAAA,OAAA;AACA,OAAA,MAAA;AACA,OAAA,KAAA;AACA,OAAA,gBAAA;AACA,OAAA,KAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,KADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,KAAA,EAAA,KAFA;AAEA,IAAA,MAAA,EAAA,MAFA;AAEA,IAAA,OAAA,EAAA,OAFA;AAEA,IAAA,KAAA,EAAA,KAFA;AAEA,IAAA,gBAAA,EAAA,gBAFA;AAGA,iBAAA;AAAA,aAAA,oCAAA;AAAA,KAHA;AAIA,oBAAA;AAAA,aAAA,uCAAA;AAAA,KAJA;AAKA,0BAAA;AAAA,aAAA,6CAAA;AAAA;AALA,GAFA;AASA,EAAA,IATA,kBASA;AACA,WAAA;AACA;AACA,MAAA,QAAA,EAAA,WAAA,CAAA,QAFA;AAGA,MAAA,SAAA,EAAA;AAHA,KAAA;AAKA,GAfA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAHA;AAIA,IAAA,cAJA,4BAIA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KANA;AAOA,IAAA,aAPA,yBAOA,KAPA,EAOA;AACA,UAAA,CAAA,KAAA,KAAA,CAAA,QAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,QAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,mBAAA,CAAA,OAAA,EAAA,KAAA,aAAA;AACA;AACA,KAZA;AAaA,IAAA,UAbA,sBAaA,KAbA,EAaA;AACA,WAAA,QAAA,CAAA,QAAA,GAAA,CAAA,KAAA,QAAA,CAAA,QAAA;;AACA,UAAA,KAAA,QAAA,CAAA,QAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,eAAA;AACA;;AACA,QAAA,MAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,KAAA,aAAA;AACA,OANA,MAMA;AACA,QAAA,MAAA,CAAA,mBAAA,CAAA,OAAA,EAAA,KAAA,aAAA;AACA;AACA,KAxBA;AAyBA,IAAA,aAzBA,2BAyBA;AACA,MAAA,aAAA,CAAA,aAAA;AACA;AA3BA;AAhBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\" :class=\"{'mobile': appState.isMobile}\">\n    <div v-if=\"appState.isMobile\" id=\"header-mask\"></div>\n    <Header :app-state=\"appState\" :toggle-menu=\"toggleMenu\"/>\n\n    <div id=\"page-menu\" ref=\"pagemenu\" class=\"noselect\" :class=\"{'page-menu-open': appState.menuOpen}\">\n      <div id=\"page-links\">\n        <span class=\"page-link-item\" v-on:click=\"openHowToModal\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>HOW-TO</span>\n        </span>\n        <a class=\"page-link-item\" href=\"http://www.european-calculator.eu/videos/\" target=\"_blank\" rel=\"noopener\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>VIDEOS</span>\n          <img src=\"/img/open_in_new-black-18dp.svg\">\n        </a>\n        <a class=\"page-link-item\" href=\"http://european-calculator.eu\" target=\"_blank\" rel=\"noopener\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>PROJECTS</span>\n          <img src=\"/img/open_in_new-black-18dp.svg\">\n        </a>\n        <!-- <a id=\"feedback\" href=\"mailto:tpe-feedback@european-calculator.eu\" :tabindex=\"appState.menuOpen ? 0 : -1\">FEEDBACK</a> -->\n        <a class=\"page-link-item\" href=\"http://www.european-calculator.eu/documentation/\" target=\"_blank\" rel=\"noopener\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n            <span>DOCUMENTATION</span>\n            <img src=\"/img/open_in_new-black-18dp.svg\">\n        </a>\n          <a class=\"page-link-item\" href=\"https://www.european-calculator.eu/literature-paper/\" target=\"_blank\" rel=\"noopener\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>SCIENCE</span>\n          <img src=\"/img/open_in_new-black-18dp.svg\">\n        </a>\n        <a class=\"page-link-item\" href=\"mailto:gino.baudry@epfl.ch\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>FEEDBACK</span>\n          <img src=\"/img/mail_outline-black-18dp.svg\">\n        </a>\n        <a class=\"page-link-item\" href=\"/legal\" :tabindex=\"appState.menuOpen ? 0 : -1\">\n          <span>LEGAL</span>\n        </a>\n      </div>\n    </div>\n\n    <transition name=\"modal-fade\" mode=\"out-in\">\n      <!--key is necessary for transition, because both parts are the same type of html element (div)-->\n      <div v-if=\"appState.mode==='main'\" id=\"main\" key=\"main\">\n        <InputsMobile v-if=\"appState.isMobile\" :app-state=\"appState\"/>\n        <Inputs v-if=\"!appState.isMobile\" :app-state=\"appState\"/>\n\n        <Outputs\n          v-if=\"appState.activeTab!==null && appState.activeSubtab!==null\"\n          :app-state=\"appState\"\n        />\n      </div>\n      <div v-if=\"appState.mode==='target'\" id=\"main\" key=\"target\">\n        <Target :app-state=\"appState\"/>\n      </div>\n\n      <div v-if=\"appState.mode==='legal'\" id=\"main\" key=\"legal\">\n        <LegalInfo :app-state=\"appState\"/>\n      </div>\n\n      <div v-if=\"appState.mode==='intro'\" id=\"main\" key=\"intro\">\n        <Intro\n          :app-state=\"appState\"\n          :open-how-to-modal=\"openHowToModal\"\n        />\n      </div>\n\n    </transition>\n\n    <transition name=\"wait-fade\">\n      <div v-if=\"appState.isMobile && (appState.mode==='target' || appState.mode==='main')\" id=\"bar-chart-cont-mob\" class=\"noselect\">\n        <EmissionsBarMobile :app-state=\"appState\"/>\n      </div>\n    </transition>\n\n    <Modal :is-open=\"appState.modalFor\" :has-close-button=\"true\" :closing-callback=\"hideInputInfo\">\n      <LeverInfoWrapper :lever-id=\"appState.modalFor\"/>\n    </Modal>\n\n    <div id=\"how-to-video\">\n      <Modal :is-open=\"howToOpen\" :has-close-button=\"true\" :closing-callback=\"closeHowToModal\">\n        <iframe width=\"560\" height=\"315\" src=\"https://www.youtube.com/embed/oGQMc2-ruFg\" frameborder=\"0\" allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>\n      </Modal>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport * as dataService from './dataService.js';\nimport * as inputsService from './inputsService.js';\nimport Header from './components/Header.vue';\nimport Inputs from './components/Inputs.vue';\nimport Outputs from './components/Outputs.vue';\nimport Target from './components/Target.vue';\nimport Intro from './components/Intro.vue';\nimport LeverInfoWrapper from './components/LeverInfoWrapper.vue';\nimport Modal from './lib/Modal.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    Header, Target,\n    Intro, Inputs, Outputs, Modal, LeverInfoWrapper,\n    'LegalInfo': () => import('./components/LegalInfo.vue'),\n    'InputsMobile': () => import('./components/InputsMobile.vue'),\n    'EmissionsBarMobile': () => import('./components/EmissionsBarMobile.vue')\n  },\n  data() {\n    return {\n      // every existing object put inside here will be transformed into a reactive getter-setter construct\n      appState: dataService.appState,\n      howToOpen: false\n    }\n  },\n  methods: {\n    closeHowToModal() {\n      this.howToOpen = false;\n    },\n    openHowToModal() {\n      this.howToOpen = true;\n    },\n    autoCloseMenu(event) {\n      if(!this.$refs.pagemenu.contains(event.target)) {\n        this.appState.menuOpen = false;\n        window.removeEventListener('click', this.autoCloseMenu);\n      }\n    },\n    toggleMenu(event) {\n      this.appState.menuOpen = !this.appState.menuOpen;\n      if(this.appState.menuOpen) {\n        if(event) {\n          //this function can also be called directly (not as an event handler)\n          event.stopPropagation();\n        }\n        window.addEventListener('click', this.autoCloseMenu);\n      } else {\n        window.removeEventListener('click', this.autoCloseMenu);\n      }\n    },\n    hideInputInfo() {\n      inputsService.hideInputInfo();\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  display: flex;\n  flex-direction: column;\n  flex-grow: 1;\n\n  /*firefox flexbox overflow bug, https://stackoverflow.com/questions/28636832/firefox-overflow-y-not-working-with-nested-flexbox*/\n  min-height: 0;\n\n  padding: 0px;\n  position: relative;\n  overflow-x: hidden; /*necessary for the page-menu*/\n  background-color: #eee;\n}\n#header-mask {\n  position: fixed;\n  top: 0px;\n  left: 0px;\n  width: 100%;\n  height: 56px;\n  backdrop-filter: blur(2px);\n  z-index: 109;\n}\n#main {\n  flex-grow: 1;\n  display: flex;\n  max-width: 100%;\n\n  /*firefox flexbox overflow bug, https://stackoverflow.com/questions/28636832/firefox-overflow-y-not-working-with-nested-flexbox*/\n  min-height: 0;\n}\n\n#page-menu {\n  position: fixed;\n  top: 0px;\n  right: -3px;\n  padding: 0px 10px 0px 20px;\n  background-color: white;\n  z-index: 110;\n  height: 150%; /*150 instead of 100 for urlbar toggle on mobile*/\n  transform: translateX(100%);\n  transition: transform 0.2s cubic-bezier(0.77,0.2,0.05,1.0);\n  border-left: 1px solid #6d6e71;\n  box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.3);\n}\n#page-menu.page-menu-open {\n  transform: translateX(calc(0% - 3px));\n}\n#page-links {\n  margin-top: 80px;\n}\n.page-link-item {\n  margin: 0px 0px 8px 0px;\n  display: flex;\n  align-items: center;\n  color: black !important;\n  transition: color 0.2s ease;\n  font-size: 16px;\n  cursor: pointer;\n}\n.page-link-item img {\n  margin-left: 8px;\n  opacity: 0.5;\n}\n#page-links a:focus {\n  text-decoration: underline;\n}\n\n#bar-chart-cont-mob {\n  position: fixed;\n  width: 100%;\n  bottom: 0px;\n  left: 0px;\n  z-index: 1;\n  box-shadow: 0px 0px 4px 0px rgba(0,0,0,0.3);\n  background-color: rgba(255,255,255,0.7);\n  backdrop-filter: blur(2px);\n}\n\n\n/* #feedback {\n  color: tomato !important;\n  animation: feedbackbling 3s infinite;\n}\n\n@keyframes feedbackbling {\n  0%  { color: black; }\n  50% { color: tomato; }\n  100% {color: black;}\n} */\n#how-to-video {\n  position: absolute;\n  height: 0px;\n}\n#how-to-video .modal-content {\n  padding-top: 35px;\n}\n#how-to-video iframe {\n  height: 100%;\n  width: 100%;\n}\n\n</style>\n"],"sourceRoot":"src"}]}