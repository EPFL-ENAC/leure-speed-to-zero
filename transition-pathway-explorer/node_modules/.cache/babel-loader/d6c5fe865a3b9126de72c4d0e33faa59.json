e from '../dataService.js';\n\nimport Loading from './Loading.vue';\n\nexport default {\n  name: 'APIStatusDialogMobile',\n  components: {\n    Loading\n  },\n  props: ['appState'],\n  computed: {\n    visible() {\n      if(this.appState.userAction.indexOf('tabChange') >= 0) {\n        return false;\n      }\n      return this.appState.responseStatus !== 200 || this.appState.waitingForAPI\n    }\n  },\n  methods: {\n    calculate() {\n      dataService.fetchFromModelAPI();\n    }\n  }\n}\n</script>\n\n<style>\n#api-status-mobile {\n  position: fixed;\n  z-index: 200;\n  width: 90vw;\n  min-height: 100px;\n  padding: 10px;\n  box-sizing: border-box;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  background-color: grey;\n  font-size: 16px;\n  color: white;\n  box-shadow: 0px 0px 8px 0px rgba(0,0,0,0.5);\n  display: flex;\n  flex-direction: column;\n}\n#api-status-mobile-loading-cont {\n  position: relative;\n  height: 70px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}