own from \"../lib/DropDown.vue\";\nimport PopUp from \"../lib/PopUp.vue\";\nexport default {\n  name: 'Outputs',\n  components: {\n    //generic subtabs\n    'OneChart': function OneChart() {\n      return import(\"./tabs/OneChart.vue\");\n    },\n    'TwoCharts': function TwoCharts() {\n      return import(\"./tabs/TwoCharts.vue\");\n    },\n    //specific subtabs\n    'EmissionsByCountry': function EmissionsByCountry() {\n      return import(\"./tabs/emissions/EmissionsByCountry.vue\");\n    },\n    'GHGSources': function GHGSources() {\n      return import(\"./tabs/emissions/GHGSources.vue\");\n    },\n    'WaterStressMap': function WaterStressMap() {\n      return import(\"./tabs/water/WaterStressMap.vue\");\n    },\n    'MineralAvailability': function MineralAvailability() {\n      return import(\"./tabs/materials/MineralAvailability.vue\");\n    },\n    'ClimateImpactChart': function ClimateImpactChart() {\n      return import(\"./tabs/ClimateImpactChart.vue\");\n    },\n    // 'EnergyFlows': () => import('./tabs/energyflows/EnergyFlows.vue'),\n    'ChangePerSector': function ChangePerSector() {\n      return import(\"./tabs/employment/ChangePerSector.vue\");\n    },\n    'EmploymentChange': function EmploymentChange() {\n      return import(\"./tabs/employment/EmploymentChange.vue\");\n    },\n    'WagesPerSkill': function WagesPerSkill() {\n      return import(\"./tabs/employment/WagesPerSkill.vue\");\n    },\n    'SectoralTrade': function SectoralTrade() {\n      return import(\"./tabs/world/SectoralTrade.vue\");\n    },\n    'EuRowTrade': function EuRowTrade() {\n      return import(\"./tabs/world/EuRowTrade.vue\");\n    },\n    'TradeComposition': function TradeComposition() {\n      return import(\"./tabs/world/TradeComposition.vue\");\n    },\n    APIStatusDialog: APIStatusDialog,\n    'APIStatusDialogMobile': function APIStatusDialogMobile() {\n      return import(\"./APIStatusDialogMobile.vue\");\n    },\n    TabNavigation: TabNavigation,\n    DropDow