{
  "kpis": [
    {
      "name": "CO2 emissions",
      "unit": "Mt",
      "route": "building-types-emissions",
      "maximize": false,
      "thresholds": {
        "warning": 0.15,
        "danger": 0.3
      },
      "info": "Total CO2 emissions in 2050 from residential buildings including space heating across all energy carriers. Lower values indicate better environmental performance and progress toward climate goals."
    },
    {
      "name": "Energy Demand for Space Heating",
      "unit": "TWh",
      "route": "energy-carriers",
      "maximize": false,
      "thresholds": {
        "warning": 1.5,
        "danger": 2.5
      },
      "info": "Total energy delivered for space heating in residential buildings, in 2050. This includes all heating sources but excludes ambient heat extracted by heat pumps, reflecting only the actual energy input required for heating."
    },
    {
      "name": "A-C class",
      "unit": "%",
      "route": "building-types-area",
      "maximize": true,
      "thresholds": {
        "warning": 60,
        "danger": 40
      },
      "info": "Share of residential floor area with high-quality thermal envelopes (categories A, B, C), in 2050. The rating reflects the building envelope's thermal resistance including insulation, windows, and thermal bridges based on CECB classification."
    },
    {
      "name": "Unrenovated Envelope Share",
      "unit": "%",
      "route": "renovation-construction",
      "maximize": false,
      "thresholds": {
        "warning": 15,
        "danger": 30
      },
      "info": "Share of residential floor area that has not undergone envelope renovation in 2050. This includes buildings whose envelope efficiency class has not improved since construction, indicating poor thermal performance."
    }
  ],
  "subtabs": [
    {
      "title": "Heating emissions",
